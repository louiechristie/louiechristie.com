    "@11ty/eleventy": "^3.0.0",
    "@playwright/test": "^1.48.1",
    "@types/node": "^22.7.7",
    "wrangler": "^3.106.0"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^6.0.0",
    "@justinribeiro/lite-youtube": "^1.7.1"
  }
